    <%= javascript_importmap_tags %>
    <%= javascript_include_tag 'application', 'data-turbo-track': 'reload' %>  </head>

  <body style="background-color: #d1cddc;">
  <%= render 'shared/header' %>
  <br>
  <%= render 'shared/flash' %>
    <%= yield %>
  </body>
</html>
